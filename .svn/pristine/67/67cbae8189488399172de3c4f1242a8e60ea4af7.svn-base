@charset 'utf-8';
@import "global.scss";
.globalNav{
    position: relative;
    .arandActivity{
        position: absolute;
        left: 420px;
        top: 79px;
        z-index: 99999;
        width: 198px;
        background-color: #f0f0f0;
        border: 1px solid #ccc;
        display: none;
        a{
            line-height: 50px;
            display: block;
            font-size: 16px;
            height: 50px;
            color: #333;
            padding-left: 18px;
            cursor: pointer;
            &:nth-of-type(1){
                border-bottom: 1px solid #ccc;
            }
        }
    }
}

