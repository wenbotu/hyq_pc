.rank-swiper-container{
    width: 1100px;
    margin: 0 auto;
    position: relative;
}
:global{
    .rank-swiper-container{
        .swiper-button-next, .swiper-button-prev{
            top: 310px;
        }
        .swiper-button-prev{
            left: -50px;
        }
        .swiper-button-next{
            right: -50px;
        }
        .swiper-container {
            width: 100%;
            //height: 300px;
            margin-left: auto;
            margin-right: auto;
        }
        .swiper-slide {
            background-size: cover;
            background-position: center;
            width: 1100px !important;
            @include flexbox2(justify,space-between,center,center);
            @include flexwrap();
            &>div{
                width: 200px;
                height: 300px;
                margin-top: 20px;
                position: relative;
                &:nth-of-type(1),
                &:nth-of-type(2),
                &:nth-of-type(3),
                &:nth-of-type(4),
                &:nth-of-type(5){
                    margin-top: 0;
                }
                img{
                    width: 200px;
                    height: 232px;
                    display: block;
                }
                div{
                    position: absolute;
                    bottom: 0;
                    width: 100%;
                    background-color: #fff;
                    padding: 10px 0;
                    span{
                        font-size: 20px;
                        color: #f35b12;
                        line-height: 1;
                        font-weight: bold;
                    }
                    p{
                        margin-top: 10px;
                        font-size: 16px;
                        color: #f35b12;
                        line-height: 1;
                    }
                }
            }
        }
        .gallery-top {
            height: 620px;
            width: 1100px;
        }
        .gallery-thumbs {
            height: 195px;
            box-sizing: border-box;
            padding: 30px 0 0 0;
            width: 100%;
        }
        .gallery-thumbs .swiper-slide {
            width: 210px;
            height: 100%;
            opacity: 0.4;
            //margin-right: 30px !important;
        }
        .gallery-thumbs .swiper-slide-active {
            opacity: 1;
        }
    }
}
