        background:url(../../style/img/arandActivity/rankKingList/titleCommon.jpg) no-repeat left center;
        color:white;
        padding-left: 23px;
    }
    .flexCss{
        display: flex;
        padding:0 8px 20px 10px;
        justify-content: space-between;
        ul{
            display: block;
            text-align: left;
            width: 432px;
            li{
                display: block;
                height: 50px;
                width: 100%;
                padding-left:80px;
                text-align: left;
                line-height: 25px;
                margin:25px 0;
                position: relative;
                &:nth-last-of-type(1){
                    margin-bottom: 0;
                }
                &.line50{
                    line-height: 50px;
                }
                &.line26{
                    line-height: 22px;
                    margin-top:-6px;
                }
                span{
                    font-size: 16px;
                }
                b{
                    display: block;
                    width: 50px;
                    height: 50px;
                    text-align: center;
                    line-height: 50px;
                    background-color:#f35b12;
                    border-radius: 50%;
                    color:white;
                    position: absolute;
                    left: 0;
                    font-size: 17px;
                    font-weight: 500;
                }
            }
            &.scaleUl{
                width: 440px;
                li{
                    margin:8px 0;
                    padding-left: 72px;
                    b.two{
                        line-height: 19px;
                        padding-top:6px;
                        box-sizing: border-box; 
                    }
                }
            }
            &.schoolSec{
                width: 490px;
            }
        }
    }
}
